# - Config file for SPIPACK

set(SPIPACK_INCLUDE_DIRS @CMAKE_INSTALL_PREFIX@/include @SPIPACK_EXTERNAL_INCLUDE_DIRS@)

set(SPIPACK_LIBRARY_DIR @CMAKE_INSTALL_PREFIX@/lib)

# These are IMPORTED targets
set(SPIPACK_LIBRARIES @SPIPACK_EXPORT_LIBRARIES@)
set(SPIPACK_EXTERNAL_LIBRARIES @SPIPACK_EXTERNAL_LIBRARIES@)

set(SPIPACK_CXX_FLAGS "@CMAKE_CXX_FLAGS@")
set(SPIPACK_C_FLAGS "@CMAKE_C_FLAGS@")
set(SPIPACK_CXX_COMPILER "@CMAKE_CXX_COMPILER@")
set(SPIPACK_C_COMPILER "@CMAKE_C_COMPILER@")
